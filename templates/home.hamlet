$maybe msg <- mensagem
    <div class="alert alert-success">
        #{msg}
        
<div class="container">
    <nav class="nav navbar">
        <ul class="navbar-nav">
            <li class="nav-item active">
                <a href=@{ProdutoR}>
                    Cadastro de Produto
            <li class="nav-item">
                <a href=@{ListaProdutoR}>
                    Listagem de Produto
            
            <li class="nav-item">
                <a href=@{UsuarioR}>
                    Cadastre-se
            $maybe _ <- logado
                <li class="nav-item">
                    <form action=@{LogoutR} method=post>
                        <input type="submit" value="Logout">
            $nothing
                <li class="nav-item">
                    <a href=@{LoginR}>
                        Login
                    
    <div class="jumbotron">
        <h1>
            _{MsgWelcome}
        
        <img src=@{StaticR imgs_pudim_jpg}>

        <button onclick="teste()" class="btn btn-primary">
            _{MsgTest}
        
<footer>
    ^{widgetFooter}